        android:layout_alignParentTop="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="473dp"
        app:srcCompat="@drawable/sharpguide" />

    <ImageView
        android:id="@+id/store"
        android:layout_width="250dp"
        android:layout_height="100dp"
